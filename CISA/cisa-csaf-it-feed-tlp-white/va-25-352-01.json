  "vulnerabilities": [
    {
      "cve": "CVE-2025-62000",
      "cwe": {
        "id": "CWE-1023",
        "name": "Incomplete Comparison with Missing Factors"
      },
      "notes": [
        {
          "category": "summary",
          "text": "BullWall Ransomware Containment does not entirely inspect a file to determine if it is ransomware. An authenticated attacker could bypass detection by encrypting a file and leaving the first four bytes unaltered. Versions 4.6.0.0, 4.6.0.6, 4.6.0.7, and 4.6.1.4 were confirmed to be affected; other versions before and after may also be affected.",
          "title": "Description"
        },
        {
          "category": "details",
          "title": "SSVC",
          "text": "SSVCv2/E:N/A:N/T:P/2025-12-18T19:55:52Z/"
        }
      ],
      "title": "BullWall Ransomware Containment incomplete file inspection",
