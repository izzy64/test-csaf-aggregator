    "vulnerabilities": [
        {
            "cve": "CVE-2021-37182",
            "cwe": {
                "id": "CWE-354",
                "name": "Improper Validation of Integrity Check Value"
            },
            "notes": [
                {
                    "category": "summary",
                    "text": "The OSPF protocol implementation in affected devices fails to verify the checksum and length fields in the OSPF LS Update messages.\n\nAn unauthenticated remote attacker could exploit this vulnerability to cause interruptions in the network by sending specially crafted OSPF packets. Successful exploitation requires OSPF to be enabled on an affected device.",
                    "title": "Summary"
                }
            ],
