  "vulnerabilities": [
    {
      "cve": "CVE-2022-45790",
      "cwe": {
        "id": "CWE-799",
        "name": "Improper Control of Interaction Frequency"
      },
      "notes": [
        {
          "category": "summary",
          "text": "Omron CJ/CS/CP series programmable logic controllers use the FINS protocol, which is vulnerable to brute-force attacks. The controllers do not enforce any rate limit on password guesses to password-protected memory regions. ",
          "title": "Vulnerability Summary"
        }
      ],
