  "vulnerabilities": [
    {
      "cve": "CVE-2025-30198",
      "cwe": {
        "id": "CWE-321",
        "name": "Use of Hard-coded Cryptographic Key"
      },
      "notes": [
        {
          "category": "summary",
          "text": "ECOVACS robot vacuums and base stations communicate via an insecure Wi-Fi network with a deterministic WPA2-PSK. The key can be easily derived from the device serial number.",
          "title": "Vulnerability Summary"
        }
      ],
      "product_status": {
