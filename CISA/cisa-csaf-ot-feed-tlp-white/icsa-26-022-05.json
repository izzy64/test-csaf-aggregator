  "vulnerabilities": [
    {
      "cve": "CVE-2025-14750",
      "cwe": {
        "id": "CWE-472",
        "name": "External Control of Assumed-Immutable Web Parameter"
      },
      "notes": [
        {
          "category": "summary",
          "text": "The web application does not sufficiently verify inputs that are assumed to be immutable but are actually externally controllable. A low-privileged user can modify the parameters and potentially manipulate account-level privileges. ",
          "title": "Vulnerability Summary"
        }
      ],
