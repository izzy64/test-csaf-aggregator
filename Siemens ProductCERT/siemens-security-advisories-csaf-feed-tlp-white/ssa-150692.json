    {
      "cve": "CVE-2021-37173",
      "cwe": {
        "id": "CWE-269",
        "name": "Improper Privilege Management"
      },
      "notes": [
        {
          "category": "summary",
          "text": "The command line interface of affected devices insufficiently restrict file read and write operations for low privileged users.\n\nThis could allow an authenticated remote attacker to escalate privileges and gain root access to the device.",
          "title": "Summary"
        }
      ],
